{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"version": 2,
	"final_space": true,
	"console_title_template": "{{.Folder}}{{if .Root}} :: root{{end}} :: {{.Shell}}",
	"blocks": [
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"type": "session",
					"style": "powerline",
					"foreground": "#29f381",
					"leading_diamond": "\uf178   ",
					"template": "{{ if .SSHSession }}\uf817 {{ end }}<#F8A13B>{{ .UserName }}</>"
				},
				{
					"type": "path",
					"style": "powerline",
					"foreground": "#29f381",
					"leading_diamond": "\uf178   ",
					"template": " {{ .Path }} ",
					"properties": {
						"style": "folder"
					}
				},
				{
					"type": "git",
					"style": "diamond",
					"foreground": "black",
					"background": "#29f366",
					"background_templates": ["{{ if or (.Working.Changed) (.Staging.Changed) }}yellow{{ end }}"],
					"leading_diamond": "\ue0b6",
					"trailing_diamond": "\ue0b4 ",
					"template": " {{ .HEAD }}{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }}{{if gt .Working.Added 0}} \uf457 {{ .Working.Added }}{{end}}{{if gt .Working.Modified 0}} \uf459 {{ .Working.Modified }}{{end}}{{if gt .Working.Deleted 0}} \uf458 {{ .Working.Deleted }}{{end}}{{if gt .Working.Untracked 0}} \uf474 {{ .Working.Untracked }}{{end}}{{ end }} ",
					"properties": {
						"add_icon": "\uf916 ",
						"branch_behind_icon": "\u2193",
						"branch_ahead_icon": "\u2191",
						"branch_gone": "\u2262",
						"branch_icon": "\uf113  ",
						"branch_identical_icon": "\u2261",
						"cherry_pick_icon": "âœ“ ",
						"commit_icon": "\u25b7 ",
						"fetch_status": true,
						"merge_icon": "\u25f4 ",
						"no_commits_icon": "[no commits]",
						"rebase_icon": "\u2c62 ",
						"tag_icon": "\u25b6 "
					}
				},
				{
					"type": "time",
					"style": "diamond",
					"foreground": "black",
					"leading_diamond": "\ue0b6",
					"trailing_diamond": "\ue0b4 ",
					"template": " \uf5ef {{ .CurrentDate | date .Format }} ",
					"background": "#67CBE7",
					"properties": {
						"time_format": "15:04:05"
					}
				},
				{
					"type": "sysinfo",
					"style": "diamond",
					"foreground": "black",
					"background": "#EF7E7E",
					"leading_diamond": "\ue0b6",
					"trailing_diamond": "\ue0b4 ",
					"template": " \uf85a {{ round .PhysicalPercentUsed .Precision }}% ",
					"properties": {
						"precision": 2
					}
				}
			]
		},
		{
			"type": "prompt",
			"alignment": "right",
			"segments": [
				{
					"type": "node",
					"style": "diamond",
					"foreground": "black",
					"background": "#8CCF7E",
					"leading_diamond": "\ue0b6",
					"trailing_diamond": "\ue0b4 ",
					"template": " \uE718 {{ .Full }} "
				},
				{
					"type": "executiontime",
					"style": "diamond",
					"foreground": "black",
					"background": "#71BAF2",
					"leading_diamond": "\ue0b6",
					"trailing_diamond": "\ue0b4 ",
					"template": " \ufa1e{{ .FormattedMs }}\u2800",
					"properties": {
						"threshold": 0,
						"style": "round"
					}
				},
				{
					"type": "battery",
					"style": "diamond",
					"foreground": "black",
					"leading_diamond": "\ue0b6",
					"trailing_diamond": "\ue0b4",
					"background_templates": ["{{if eq \"Charging\" .State.String}}#96D988{{end}}", "{{if eq \"Discharging\" .State.String}}#F4D67A{{end}}", "{{if eq \"Full\" .State.String}}green{{end}}"],
					"template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }} ",
					"properties": {
						"discharging_icon": "\uE231 ",
						"charging_icon": "\uE234 ",
						"charged_icon": "\uE22F "
					}
				}
			]
		},
		{
			"type": "prompt",
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"type": "text",
					"style": "plain",
					"foreground": "#29f366",
					"template": " \uf178 "
				}
			]
		}
	],
	"osc99": true
}
